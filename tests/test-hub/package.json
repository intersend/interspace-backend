{
  "name": "@interspace/test-hub",
  "version": "1.0.0",
  "description": "Military-grade API testing hub for comprehensive endpoint validation",
  "main": "cli.ts",
  "scripts": {
    "test": "ts-node cli.ts",
    "test:quick": "ts-node cli.ts --mode quick",
    "test:standard": "ts-node cli.ts --mode standard",
    "test:comprehensive": "ts-node cli.ts --mode comprehensive",
    "test:auth": "ts-node cli.ts --suites Authentication",
    "test:security": "ts-node cli.ts --tags security",
    "test:help": "ts-node cli.ts --help",
    "list": "ts-node cli.ts list"
  },
  "keywords": [
    "testing",
    "api",
    "integration",
    "e2e",
    "test-automation"
  ],
  "author": "Interspace",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "p-limit": "^3.1.0",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "date-fns": "^2.30.0",
    "otplib": "^12.0.1",
    "ethers": "^6.9.0",
    "siwe": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}