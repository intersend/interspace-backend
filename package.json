{
  "name": "interspace-backend",
  "version": "1.0.1",
  "description": "Backend API for Interspace MVP wallet with SmartProfiles, ERC-7702 session wallets, and iPhone-style app management",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test:unit": "NODE_ENV=test jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --config jest.integration.config.js",
    "test:e2e": "NODE_ENV=test jest --config jest.config.js --testPathPattern=e2e",
    "test:watch": "NODE_ENV=test jest --config jest.config.js --watch",
    "test:coverage": "NODE_ENV=test jest --config jest.config.js --coverage",
    "test:ci": "NODE_ENV=test jest --config jest.config.js --ci --coverage --watchAll=false",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:test:reset": "NODE_ENV=test prisma migrate reset --force",
    "prisma:test:migrate": "NODE_ENV=test prisma migrate deploy"
  },
  "keywords": [
    "web3",
    "wallet",
    "crypto",
    "erc-7702",
    "session-wallet",
    "smartprofiles"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@orb-labs/orby-core": "^0.0.30",
    "@orb-labs/orby-ethers6": "^0.0.18",
    "@prisma/client": "^6.8.2",
    "@silencelaboratories/dkls-wasm-ll-node": "^1.2.0",
    "@silencelaboratories/ecdsa-tss": "^1.1.3",
    "@simplewebauthn/server": "^13.1.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/joi": "^17.2.2",
    "@types/node": "^22.15.21",
    "@types/socket.io": "^3.0.1",
    "apple-signin-auth": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.3",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.8.2",
    "rate-limiter-flexible": "^7.1.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
