{
  "name": "interspace-backend",
  "version": "1.0.1",
  "description": "Backend API for Interspace MVP wallet with SmartProfiles, ERC-7702 session wallets, and iPhone-style app management",
  "main": "index.js",
  "scripts": {
    "prebuild": "node scripts/check-production-build.js",
    "build": "tsc",
    "build:js": "tsc --noEmit false --skipLibCheck true",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "node scripts/start-prod.js",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test:unit": "NODE_ENV=test jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --config jest.integration.config.js",
    "test:e2e": "NODE_ENV=test jest --config jest.e2e.config.js",
    "test:e2e:docker": "./scripts/run-e2e-tests.sh",
    "test:e2e:local": "NODE_ENV=test jest --config jest.e2e.config.js --runInBand",
    "test:watch": "NODE_ENV=test jest --config jest.config.js --watch",
    "test:coverage": "NODE_ENV=test jest --config jest.config.js --coverage",
    "test:ci": "NODE_ENV=test jest --config jest.config.js --ci --coverage --watchAll=false",
    "docker:e2e:up": "docker-compose -f docker-compose.e2e.yml up -d",
    "docker:e2e:down": "docker-compose -f docker-compose.e2e.yml down -v",
    "docker:e2e:logs": "docker-compose -f docker-compose.e2e.yml logs -f",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:test:reset": "NODE_ENV=test prisma migrate reset --force",
    "prisma:test:migrate": "NODE_ENV=test prisma migrate deploy",
    "test:orby:connection": "ts-node -r tsconfig-paths/register scripts/test-orby-connection.ts",
    "test:orby:endpoints": "ts-node -r tsconfig-paths/register scripts/test-orby-endpoints.ts",
    "test:fund-wallets": "ts-node -r tsconfig-paths/register tests/e2e/scripts/fund-test-wallets.ts",
    "test:check-balances": "ts-node -r tsconfig-paths/register tests/e2e/scripts/check-testnet-balances.ts",
    "fund-wallets": "ts-node -r tsconfig-paths/register scripts/fund-test-wallets.ts",
    "patch:orby": "node scripts/patch-orby.js",
    "postinstall": "node scripts/patch-orby.js",
    "test:sendgrid": "ts-node -r tsconfig-paths/register scripts/test-sendgrid.ts",
    "test:flat-identity": "./scripts/test-flat-identity.sh",
    "check:clusters": "tsx scripts/check-account-clusters.ts",
    "fix:mpc-links": "tsx scripts/fix-missing-mpc-links.ts",
    "debug:profile": "tsx scripts/debug-profile-lookup.ts",
    "test:flat-identity:coverage": "NODE_ENV=test jest --config jest.config.js --coverage --testPathPattern='(flatIdentityModel|accountService)'"
  },
  "keywords": [
    "web3",
    "wallet",
    "crypto",
    "erc-7702",
    "session-wallet",
    "smartprofiles"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@orb-labs/orby-core": "^0.0.30",
    "@orb-labs/orby-ethers6": "^0.0.18",
    "@prisma/client": "^6.10.1",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@sendgrid/mail": "^8.1.5",
    "@silencelaboratories/dkls-wasm-ll-node": "^1.2.0",
    "@silencelaboratories/ecdsa-tss": "^1.1.3",
    "@simplewebauthn/server": "^13.1.1",
    "@simplewebauthn/types": "^12.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.23",
    "@types/joi": "^17.2.2",
    "@types/node": "^22.15.21",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "apple-signin-auth": "^2.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.4",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "otplib": "^12.0.1",
    "prisma": "^6.8.2",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^7.1.1",
    "redis": "^5.5.6",
    "siwe": "^3.0.0",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/sendgrid": "^2.0.31",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
