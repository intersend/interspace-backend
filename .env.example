# Database
DATABASE_URL="file:./dev.db"

# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_EXPIRES_IN=30d

# Thirdweb Configuration
THIRDWEB_CLIENT_ID=your-thirdweb-client-id
THIRDWEB_SECRET_KEY=your-thirdweb-secret-key

# Orby Configuration
ORBY_INSTANCE_PRIVATE_API_KEY=your-orby-private-key
ORBY_INSTANCE_PUBLIC_API_KEY=your-orby-public-key
ORBY_APP_NAME=interspace
ORBY_PRIVATE_INSTANCE_URL=https://your-orby-instance-url.com

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# Chain Configuration
DEFAULT_CHAIN_ID=1
SUPPORTED_CHAINS=1,10,137,42161,8453

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Session Wallet Factory Addresses (per chain)
SESSION_WALLET_FACTORY_1=0x...
SESSION_WALLET_FACTORY_137=0x...
SESSION_WALLET_FACTORY_10=0x...
SESSION_WALLET_FACTORY_42161=0x...
SESSION_WALLET_FACTORY_8453=0x...
